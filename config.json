{
    "llm": {
      "openai": {
        "base_url": null,
        "api_key": null,
        "connect_timeout": 60.0,
        "read_timeout": 10.0
      }
    },
  
    "agent": {
      "default": {
        "provider": "openai",
        "model": "gpt-4-0125-preview",
        "temperature": 0.5
      },
      "CodeMonkey.describe_files": {
        "provider": "openai",
        "model": "gpt-3.5-turbo",
        "temperature": 0.0
      }
    },
  
    "log": {
      "level": "DEBUG",
      "format": "%(asctime)s %(levelname)s [%(name)s] %(message)s",
      "output": "pythagora.log"
    },
    "db": {
      "url": "sqlite+aiosqlite:///pythagora.db",
      "debug_sql": false
    },
    "ui": {
      "type": "plain"
    },
    "fs": {
      "type": "local",
      "workspace_root": "workspace",
      "ignore_paths": [
        ".git",
        ".gpt-pilot",
        ".idea",
        ".vscode",
        ".next",
        ".DS_Store",
        "__pycache__",
        "site-packages",
        "node_modules",
        "package-lock.json",
        "venv",
        "dist",
        "build",
        "target",
        "*.min.js",
        "*.min.css",
        "*.svg",
        "*.csv",
        "*.log",
        "go.sum"
      ],
      "ignore_size_threshold": 50000
    }
  }